@model IEnumerable<Gestion_Del_Presupuesto.Models.Historial_Actividad>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Accion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Accion)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Usuarios.Id_Usuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Historial_Actividad">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Historial_Actividad">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Historial_Actividad">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
